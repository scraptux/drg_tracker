      "vue/no-deprecated-router-link-tag-prop": "off",
      "no-undef": "off",
      "vue/no-use-v-if-with-v-for": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
